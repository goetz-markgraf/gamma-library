let count-increase list =
    let current! = 10000

    list |> fold 0 [ acc i ->
        let increase = i > current!
        set current! = i
        acc + (increase ? 1 : 0) ]



print (input |> count-increase)

read-lines "src/test/resources/advent-of-code/day1.input" |> count-increase
